package DigitalInovationOne.gof;

/** 
 * singletron "pregui√ßoso".
 * 
 * @author cristopheroan
*/
public class singletronLazy{
    public static singletronLazy instancia;

    private singletronLazy() {
        super();
    }

    public static singletronLazy gatInstancia() {
        if(instancia == null ) {
            instancia = new singletronLazy();
        }
        return instancia;
    }
}
